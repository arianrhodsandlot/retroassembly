{
  "$schema": "node_modules/wrangler/config-schema.json",
  "assets": {
    "binding": "ASSETS",
    "directory": "./dist/assets",
    "html_handling": "drop-trailing-slash",
    "not_found_handling": "404-page",
    "run_worker_first": false
  },
  "compatibility_date": "2025-07-09",
  "compatibility_flags": [
    "nodejs_compat",
    "no_handle_cross_request_promise_resolution"
  ],
  "d1_databases": [
    {
      "binding": "DB_LIBRARY",
      "database_id": "${obj.DB_LIBRARY_DATABASE_ID ?? '-'}",
      "database_name": "retroassembly_library",
      "migrations_dir": "src/databases/migrations"
    }
  ],
  "main": "./src/server/workerd.ts",
  "name": "retroassembly",
  "observability": {
    "enabled": true,
    "head_sampling_rate": 1
  },
  "placement": {
    "mode": "smart"
  },
  "r2_buckets": [
    {
      "binding": "BUCKET",
      "bucket_name": "retroassembly",
      "preview_bucket_name": "retroassembly"
    }
  ],
  "vars": {
    "MSLEUTH_HOST": "${obj.MSLEUTH_HOST ?? ''}",
    "STORAGE_DOMAIN": "${obj.STORAGE_DOMAIN ?? ''}",
    "SUPABASE_ANON_KEY": "${obj.SUPABASE_ANON_KEY ?? ''}",
    "SUPABASE_URL": "${obj.SUPABASE_URL ?? ''}"
  },
  "upload_source_maps": true
}
